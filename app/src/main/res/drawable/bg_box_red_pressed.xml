<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="3dp"
        android:right="3dp"
        android:left="3dp"
        android:bottom="3dp">
        <shape>
            <size
                android:width="10dp"
                android:height="10dp"/>
            <corners android:radius="3dp" />
            <solid android:color="#FF8A65"/>
            <stroke
                android:width="1dp"
                android:color="#FF8A65"/>
            <padding
                android:bottom="10dp"
                android:top="10dp"
                android:left="10dp"
                android:right="10dp" />
        </shape>
    </item>
</layer-list>